{
  "name": "os-portfolio",
  "version": "1.10.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ci": "npm start & wait-on http://localhost:4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "./node_modules/.bin/karma start karma.conf.js",
    "start:pwa": "ng build && http-server -p 8080 -c-1 dist/os-portfolio",
    "release": "standard-version",
    "prepare": "husky install",
    "lint": "npx eslint src --fix",
    "version": "node -p \"require('./package.json').version\"",
    "zip": "node zip.ts",
    "package": "npm run build:ssr && npm run zip",
    "serve:ssr:os-portfolio": "node dist/os-portfolio/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.3",
